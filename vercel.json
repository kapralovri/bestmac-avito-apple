{
  "functions": {
    "api/sell.ts": {
      "runtime": "edge"
    },
    "api/buyout/estimate.ts": {
      "includeFiles": "public/data/**"
    }
  },
  "rewrites": [
    { "source": "/sell", "destination": "/api/sell" },
    { "source": "/sell/", "destination": "/api/sell" },
    { "source": "/api/buyout/estimate", "destination": "/api/buyout/estimate" },
    { "source": "/((?!.*\\.).*)", "destination": "/index.html" }
  ]
}
